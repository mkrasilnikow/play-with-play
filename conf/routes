# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /get/:name        controllers.BrandController.getById(name: String)
POST    /create           controllers.BrandController.create(request: Request)
# POST    /update           controllers.BrandController.update(request: Request)
# DELETE  /delete/:name     controllers.BrandController.delete(name: String)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
